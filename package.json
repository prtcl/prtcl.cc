{
  "private": true,
  "name": "prtcl.cc",
  "version": "0.0.0",
  "description": "Code for prtcl.cc",
  "repository": {
    "type": "git",
    "url": "https://github.com/prtcl/prtcl.cc.git"
  },
  "main": "src/main.tsx",
  "author": "Cory O'Brien <cory@prtcl.cc>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode=production --node-env=production",
    "clean": "rm -r ./node_modules | true ; rm -r ./dist | true",
    "dev:serve": "webpack serve --mode=development --live-reload",
    "dev:watch": "webpack --watch --mode=development --node-env=development",
    "dev": "concurrently \"npm run dev:watch\" \"npm run dev:serve\"",
    "lint": "tsc --noEmit --pretty --skipLibCheck && eslint src",
    "panda": "panda codegen --clean",
    "prettier": "prettier src --write",
    "setup": "npm run clean && npm ci && npm run panda"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "plonk": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-use": "^17.5.0"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.39.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "concurrently": "^8.2.2",
    "css-loader": "^6.11.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.5.3",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.5.11",
    "prettier": "^3.2.5",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.4.2",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
