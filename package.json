  "scripts": {
    "clean": "rm -r ./node_modules ; rm -r ./static/dist",
    "dev": "$(npm bin)/parallelshell 'npm run webpack:watch' 'npm run start'",
    "server:dev": "NODE_ENV=development PORT=3000 HEALTH_CHECK=true DEBUG=supermoon:* nodemon ./index.js",
    "server": "NODE_ENV=production PORT=9999 HEALTH_CHECK=true node ./index.js",
    "start": "cd www && $(npm bin)/static -a 0.0.0.0",
    "webpack:watch": "$(npm bin)/webpack --watch",
    "webpack": "$(npm bin)/webpack"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
