    "css-loader": "^0.28.9",
    "eslint": "^4.15.0",
    "eslint-plugin-babel": "^4.1.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "node-static": "^0.7.10",
    "nodemon": "^1.14.11",
    "parallelshell": "^3.0.2",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0"
