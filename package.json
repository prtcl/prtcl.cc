    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "node-static": "^0.7.10",
    "nodemon": "^1.14.11",
    "parallelshell": "^3.0.2",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "plonk": "^1.1.0"
  }
