    "webpack:watch": "$(npm bin)/webpack --watch",
    "webpack": "$(npm bin)/webpack"
  },
  "devDependencies": {
    "buble": "^0.18.0",
    "buble-loader": "^0.4.1",
    "css-loader": "^0.28.9",
